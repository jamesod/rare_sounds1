<h1>Listing musics</h1>

<table>
  <tr>
    <th>Release</th>
    <th>Artist</th>
    <th>Genre</th>
    <th>Format</th>
    <th>Date</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @musics.each do |music| %>
  <tr>
    <td><%= music.release %></td>
    <td><%= music.artist %></td>
    <td><%= music.genre %></td>
    <td><%= music.format %></td>
    <td><%= music.date %></td>
    <td><%= music.price %></td>
    <td><%= link_to 'Show', music %></td>
    <td><%= link_to 'Edit', edit_music_path(music) %></td>
    <td><%= link_to 'Destroy', music, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Music', new_music_path %>
