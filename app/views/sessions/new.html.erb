<div class="center">
  <div class="header"><img src="/images/logo.png" width="251" height="92" align="left"/>

    <div class="navbar"></div>

  </div>

  <h1>Login</h1>

  <p id="notice"><%= notice %></p>
  <% form_tag sessions_path do %>
      <div class="field"> <%= label_tag :email %>
        <br/> <%= text_field_tag :email %>
      </div>
      <div class="field">
        <%= label_tag :password %>
        <br/>
        <%= password_field_tag :password %> </div>
      <div class="actions"> <%= submit_tag 'Login' %>
      </div>
  <% end %>


</div>